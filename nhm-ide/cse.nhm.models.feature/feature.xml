<?xml version="1.0" encoding="UTF-8"?>
<feature id="cse.nhm.models.feature" label="NHM Model Versions" version="1.0.0.qualifier" provider-name="CSE">
  <description url="http://www.example.com/description">
     Depends on the implementation bundles for the NHM and its manual. Installing this feature will allow the NHM user interface to run scenarios, validate, and provide help.
   </description>
  <copyright url="http://www.example.com/copyright">
     Copyright (C) DECC 2015
   </copyright>
  <license url="http://www.example.com/license">
     The license for the NHM core code belongs here.
  </license>
  <!--
      What does match mean below?

      perfect - dependent plug-in version must match exactly the
      specified version. If "patch" is "true", "perfect" is assumed
      and other values cannot be set.

      equivalent - dependent plug-in version must be at least at the
      version specified, or at a higher service level (major and minor
      version levels must equal the specified version).

      compatible - dependent plug-in version must be at least at the
      version specified, or at a higher service level or minor level
      (major version level must equal the specified version).

      greaterOrEqual - dependent plug-in version must be at least at
      the version specified, or at a higher service, minor or major
      level.
  -->
   <requires>
     <import plugin="uk.org.cse.nhm.manual" version="0.0.0" match="greaterOrEqual"/>
     <import plugin="uk.org.cse.nhm.bundle.impl" version="7.2.0" match="equivalent"/>

     <!-- <import plugin="uk.org.cse.nhm.manual" version="7.0.7" match="greaterOrEqual"/> -->
     <import plugin="uk.org.cse.nhm.bundle.impl" version="7.0.7.Rv-7_0_7" match="perfect"/>
     <import plugin="uk.org.cse.nhm.bundle.impl" version="7.0.6.Rv-7_0_6" match="perfect"/>
     <import plugin="uk.org.cse.nhm.bundle.impl" version="6.5.5.Rv-6_5_5_dirty" match="perfect"/>
     <import plugin="uk.org.cse.nhm.bundle.impl" version="6.5.2" match="equivalent"/>
     <import plugin="uk.org.cse.nhm.bundle.impl" version="6.5.1" match="equivalent"/>
     <import plugin="uk.org.cse.nhm.bundle.impl" version="6.4.0" match="equivalent"/>
     <import plugin="uk.org.cse.nhm.bundle.impl" version="6.3.2" match="equivalent"/>
  </requires>
</feature>
