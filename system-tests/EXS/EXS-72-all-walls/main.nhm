(scenario
    start-date: 2015
    end-date:   2015
    stock-id:   ../../EHS2012.json.gz
    quantum:    1000000
    
    (on.dates
        (scenario-start)
        
        (aggregate
            name: counts
            
            (aggregate.count name:count)
            
            (aggregate.where
                name: all-walls-unins-cav
                (house.all-walls has-construction: AnyCavity
                                 has-cavity-insulation: false))
            
            (aggregate.where
                name: any-walls-unins-cav
                (all (house.any-wall has-construction: AnyCavity)
                     (none (house.any-wall has-cavity-insulation: true))))
            
            )))