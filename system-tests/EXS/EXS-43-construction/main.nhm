(scenario
 start-date: 0014
 end-date:   2010
 stock-id:   ../../stock.json
 quantum:    1000000

 (on.construction
  (assert
   (= (summarize aggregate.count affected-houses)
      expected-build)))
 
 (on.dates
  regularly
  (aggregate name: totals
             (aggregate.count name:count))

  (assert
   capture: [expected (summarize aggregate.count) sim.year]
   (= expected (summarize aggregate.count))))

